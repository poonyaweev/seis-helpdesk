<!DOCTYPE html>
<html>
<head>
    <title>Helpdesk Tickets</title>
</head>
<body>
    <h1>Helpdesk Tickets</h1>
    <a href="/create">Create New Ticket</a>
    <table>
        <thead>
            <tr>
                <th>Ticket #</th>
                <th>Title</th>
                <th>Status</th>
                <th>Created At</th>
                <th>Actions</th>
                <th>Images</th>
            </tr>
        </thead>
        <tbody>
            <% tickets.forEach(ticket => { %>
                <tr>
                    <td><%= ticket.ticketNumber %></td>
                    <td><%= ticket.title %></td>
                    <td><%= ticket.status %></td>
                    <td><%= ticket.createdAt %></td>
                    <td><a href="/update/<%= ticket._id %>">Update</a></td>
                    <td><img src="/image/<%= ticket._id %>" alt="Ticket Image"></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>